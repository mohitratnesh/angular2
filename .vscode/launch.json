    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceRoot}/src/",
            "trace":true,
            "runtimeArgs":[
                "--new-window", "--user-data-dir=c:/Windows","--remote-debugging-port=9222"
            ],
            "sourceMaps": true
        }
    ]
}